			orientation: "horizontal"
			size_hint_y: None
			height: 40
			padding: 0,0,0,12
			Label:
				text: "Email id:* "
			TextInput:
				id: email
				multiline: False
				focus: True
				on_text_validate: contact.focus= True
			Label:
				id: mail
				markup: True
