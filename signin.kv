<SigninWindow>:
	
	id: main_win
	name: "main_win"
	orientation: "vertical"
	spacing: 10
	space_x: self.size[0]/3
	email:email
	pwd:pwd
	info:info
	
	canvas.before:
		Rectangle:
			pos: self.pos
			size: self.size
			source:"yel.jpg"
	 
	BoxLayout:
		size_hint_y: None
		height: 50
		canvas.before:
			Color:
				rgba:(1,2,3,0.4)
			Rectangle:
				size: self.size
				pos: self.pos
		Label:
			text: "LOGIN SCREEN"
			bold: True
			size_hint_x: 0.9
		
	
	Label:
		id: info
		text: ''
		markup: True
		size_hint_y: None
		height: 20
		
	BoxLayout:
		orientation: "vertical"
		padding: main_win.space_x,10 
		BoxLayout:
			orientation: "horizontal"
			size_hint_y: None
			spacing: 10
			padding: 0,0,0,12
			height: 40
			Label:
				id: username_field
				text: "Email ID: "
			TextInput:
				id: email
				multiline: False
				focus: True
				on_text_validate: pwd.focus= True
		BoxLayout:
		
			orientation: "horizontal"
			size_hint_y: None
			spacing: 10
			padding: 0,0,0,12
			height: 40
			Label:
				id: pwd_field1
				text: "Password: "
			TextInput:
				id: pwd
				multiline: False
				password: True
				focus: True
				on_text_validate:root.loginBtn()

		Label:
			id: sp1
			size_hint_y: None
			height: 40
		Button:
			size_hint_y: None
			height: 40
			background_color:(1,2,3,0.4)
			text: "Sign in"
			on_release:
				root.loginBtn()
				
			
		Button:
			size_hint_y: None
			height: 40
			background_color:(1,2,3,0.4)
			text: "Make a new account"
			on_release:
				root.info.text = ""
				app.root.current = "second" 
				root.manager.transition.direction= "left"
		Label:
			id: sp2

<Signup>:
	name: "second"
	id: reg_win
	orientation: "vertical"
	spacing: 10
	space_x: self.size[0]/4
	name1:name1
	year:year
	contact:contact
	email:email
	branch:branch
