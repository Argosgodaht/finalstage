		BoxLayout:
			orientation: "horizontal"
			size_hint_y: None
			padding: 0,0,0,12
			height: 40
			Label:
				text: "Contact no.: "
			TextInput:
				id: contact
				multiline: False
				focus: True
				on_text_validate: root.submit()
			Label:
				id: con
				markup: True
				text: ""
		Label:
			text: ""
			height: 50
			size_hint_y: None
		
		BoxLayout:
			orientation: "horizontal"
			size_hint_y: None
			padding: 0,0,0,12
			height: 40
			Label:
				text: ""
				size_hint_x: 0.2
			Button:
				id: reg
				size_hint_y: None
				size_hint_x: 0.5
				height: 40
				text: "Reset"
				background_color:(1,2,3,0.4)
				on_release:
					root.re()
			Label:
				text: ""
				size_hint_x: 0.2
				
		BoxLayout:
			orientation: "horizontal"
			size_hint_y: None
			padding: 0,0,0,12
			height: 40
